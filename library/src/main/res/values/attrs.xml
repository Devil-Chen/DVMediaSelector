<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="iconSize" format="dimension"/>
    <attr name="iconMargin" format="dimension"/>
    <attr name="iconSrc" format="reference"/>
    <attr name="iconLeft" format="reference"/>
    <attr name="iconRight" format="reference"/>
    <attr name="duration_max" format="integer"/>
    <declare-styleable name="DVCameraView">
        <attr name="iconSize"/>
        <attr name="iconMargin"/>
        <attr name="iconSrc"/>
        <attr name="iconLeft"/>
        <attr name="iconRight"/>
        <attr name="duration_max"/>
    </declare-styleable>
    <declare-styleable name="JCameraView">
        <attr name="iconSize"/>
        <attr name="iconMargin"/>
        <attr name="iconSrc"/>
        <attr name="iconLeft"/>
        <attr name="iconRight"/>
        <attr name="duration_max"/>
    </declare-styleable>

    <!--ImageView的宽高根据设置的比例动态适配高度-->
    <declare-styleable name="RatioImageView">
        <!--图片间隔-->
        <attr name="ratio" format="float"/>
    </declare-styleable>
</resources>